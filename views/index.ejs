<!DOCTYPE html>
<html ng-app="app">
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body ng-controller="UploadController">
    <input type="file" ng-model="aName" name="upload" multiple="multiple" onchange="angular.element(this).scope().fileChanged(this)">
    <input type="text" ng-model="description">
    <button ng-click="save()">Save</button></br>
    content: <textarea ng-model="fileContent"></textarea></br>

    <!-- Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular-route.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular-resource.min.js"></script>

    <script src="/javascripts/index.js"></script>


  </body>
</html>
